{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "Frontend/build" }
    },
    {
      "src": "Cackend/**/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/Backend/(.*)",
      "dest": "/Backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Backend/$1"
    }
  ]
}
